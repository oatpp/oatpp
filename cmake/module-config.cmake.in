@PACKAGE_INIT@

if(NOT TARGET oatpp::@OATPP_MODULE_NAME@)
    include("${CMAKE_CURRENT_LIST_DIR}/@OATPP_MODULE_NAME@Targets.cmake")
endif()

set_and_check(@OATPP_MODULE_NAME@_INCLUDE_DIRS "@CMAKE_INSTALL_FULL_INCLUDEDIR@/oatpp-@OATPP_MODULE_VERSION@/@OATPP_MODULE_NAME@/")
set_and_check(@OATPP_MODULE_NAME@_LIBRARIES_DIRS "@CMAKE_INSTALL_FULL_LIBDIR@/oatpp-@OATPP_MODULE_VERSION@/")

set(@OATPP_MODULE_NAME@_LIBRARIES @OATPP_MODULE_LIBRARIES@)
set(OATPP_BASE_DIR "@CMAKE_INSTALL_FULL_INCLUDEDIR@/oatpp-@OATPP_MODULE_VERSION@/")
